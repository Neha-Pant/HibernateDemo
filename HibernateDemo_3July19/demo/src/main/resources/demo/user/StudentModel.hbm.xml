<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="demo.user.StudentModel" table="Student">
		<id name="id" type="int">
			<column name="id" precision="5" scale="0" />
			<generator class="assigned" />
		</id>
		<property name="name" type="string" column="name"/>
		<!-- 	<column name="name" length="20" not-null="true" />
		</property> -->
		<property name="course" type="string" column="course"/>
		<!-- 	<column name="course" length="20" not-null="true" />
		</property> -->
		<property name="marks" type="int" column="marks"/>
			<!-- <column name="marks" length="5" not-null="true" />
		</property> -->
	</class>
</hibernate-mapping>
